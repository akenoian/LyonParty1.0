<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".PostEvent"
    tools:showIn="@layout/activity_post_event">

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="21dp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="21dp"
        app:srcCompat="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/textViewLabel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView1"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="107dp"
        android:layout_marginLeft="107dp"
        android:text="Event Name: " />

    <TextView
        android:id="@+id/textViewLabel2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView1"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="111dp"
        android:layout_marginLeft="111dp"
        android:layout_marginTop="33dp"
        android:text="Host:" />

    <TextView
        android:id="@+id/postEventName3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textViewLabel2"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="111dp"
        android:layout_marginLeft="111dp"
        android:layout_marginTop="35dp"
        android:text="Date:" />

    <TextView
        android:id="@+id/textViewLabel4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textViewLabel2"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="245dp"
        android:layout_marginLeft="245dp"
        android:layout_marginTop="35dp"
        android:text="Time:" />

    <EditText
        android:id="@+id/editText1"
        android:layout_width="176dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="19dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="18dp"
        android:ems="10"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="91dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textViewLabel4"
        android:layout_alignParentTop="true"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="42dp"
        android:ems="10"
        android:inputType="date" />

    <EditText
        android:id="@+id/editTextEmail"
        android:layout_width="76dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textViewLabel1"
        android:layout_alignParentTop="true"
        android:layout_marginStart="176dp"
        android:layout_marginTop="69dp"
        android:ems="10"
        android:inputType="time" />

    <TextView
        android:id="@+id/postEventName5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView1"
        android:layout_alignParentTop="true"
        android:layout_marginTop="137dp"
        android:text="Info: " />

    <TextView
        android:id="@+id/postEventName6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView1"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="182dp"
        android:text="Tags:" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="303dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editTextEmail"
        android:layout_alignEnd="@+id/textViewLabel4"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="-82dp"
        android:ems="10"
        android:inputType="textMultiLine" />

    <ToggleButton
        android:id="@+id/toggleButtonTag1"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-68dp"
        android:layout_marginBottom="257dp"
        android:layout_toEndOf="@+id/imageView1"
        android:text="ToggleButton"
        android:textOff="free food"
        android:textOn="FREE FOOD" />

    <ToggleButton
        android:id="@+id/toggleButtonTag"
        android:layout_width="wrap_content"
        android:layout_height="59dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-73dp"
        android:layout_marginBottom="131dp"
        android:layout_toEndOf="@+id/imageView1"
        android:text="ToggleButton"
        android:textOff="drinks"
        android:textOn="DRINKS" />

    <ToggleButton
        android:id="@+id/toggleButtonTag2"
        android:layout_width="82dp"
        android:layout_height="59dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-69dp"
        android:layout_marginBottom="197dp"
        android:layout_toEndOf="@+id/imageView1"
        android:text="ToggleButton"
        android:textOff="free coffee"
        android:textOn="FREE COFFEE" />

    <ToggleButton
        android:id="@+id/toggleButtonTag4"
        android:layout_width="82dp"
        android:layout_height="59dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-69dp"
        android:layout_marginBottom="67dp"
        android:layout_toEndOf="@+id/imageView1"
        android:text="ToggleButton"
        android:textOff="live music"
        android:textOn="LIVE MUSIC" />

    <ToggleButton
        android:id="@+id/toggleButtonTag5"
        android:layout_width="wrap_content"
        android:layout_height="58dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="29dp"
        android:layout_marginBottom="256dp"
        android:layout_toEndOf="@+id/imageView1"
        android:text="ToggleButton"
        android:textOff="lecture"
        android:textOn="LECTURE" />

    <ToggleButton
        android:id="@+id/toggleButtonTag6"
        android:layout_width="wrap_content"
        android:layout_height="58dp"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="29dp"
        android:layout_marginBottom="197dp"
        android:layout_toEndOf="@+id/imageView1"
        android:text="ToggleButton"
        android:textOff="political event"
        android:textOn="POLITICAL EVENT" />
</RelativeLayout>